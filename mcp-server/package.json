    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^2.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "ui-agent",
    "ai-agent",
    "model-context-protocol",
    "ui-generation",
