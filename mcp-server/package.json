{
  "name": "@anthropic-ui/ui-agent-mcp",
  "version": "1.0.0",
  "description": "MCP Server for UI Agent - AI-powered UI generation via Model Context Protocol",
  "type": "module",
  "main": "dist/server.js",
  "bin": {
    "ui-agent-mcp": "./dist/server.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
